{
  "name": "review-master",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "npx prettier --write ./src"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "axios": "^0.24.0",
    "bootstrap": "5.1.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.0.2",
    "next": "12.0.2",
    "prettier": "^2.4.1",
    "react": "17.0.2",
    "react-bootstrap": "^2.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "react-select": "^5.2.1"
  },
  "devDependencies": {
    "@types/node": "16.11.6",
    "@types/react": "17.0.34",
    "eslint": "7",
    "eslint-config-next": "12.0.2",
    "eslint-config-prettier": "^8.3.0",
    "typescript": "4.4.4"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --ext .tsx --ext .ts src/ --fix"
    ],
    "./src/**": [
      "prettier --write ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
